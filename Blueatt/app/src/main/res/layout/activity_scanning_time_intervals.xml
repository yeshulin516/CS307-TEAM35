<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.moonsunhwang.blueatt.Scanning_time_intervals">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="236dp"
        android:layout_height="42dp"
        android:text="Please select the time interval\nto scan attendance"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="29dp" />

    <Button
        android:id="@+id/twenty_seconds"
        android:layout_width="120dp"
        android:layout_height="88dp"
        android:text="20 Seconds"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="112dp" />

    <Button
        android:id="@+id/fourty_seconds"
        android:layout_width="120dp"
        android:layout_height="88dp"
        android:text="40 Seconds"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="201dp"
        tools:layout_editor_absoluteY="112dp" />

    <Button
        android:id="@+id/sixty_seconds"
        android:layout_width="120dp"
        android:layout_height="88dp"
        android:text="60 Seconds"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="229dp" />

    <Button
        android:id="@+id/eighty_seconds"
        android:layout_width="120dp"
        android:layout_height="88dp"
        android:text="80 Seconds"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="201dp"
        tools:layout_editor_absoluteY="229dp" />

    <Button
        android:id="@+id/hundred_seconds"
        android:layout_width="120dp"
        android:layout_height="88dp"
        android:text="100 Seconds"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="345dp" />

    <Button
        android:id="@+id/hunderedAndTwenty_seconds"
        android:layout_width="120dp"
        android:layout_height="88dp"
        android:text="120 Seconds"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="201dp"
        tools:layout_editor_absoluteY="345dp" />

</android.support.constraint.ConstraintLayout>
