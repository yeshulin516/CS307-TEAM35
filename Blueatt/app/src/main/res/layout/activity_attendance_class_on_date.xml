<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.moonsunhwang.blueatt.Attendance_class_on_date"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/date_TextView"
        android:layout_width="185dp"
        android:layout_height="20dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/total_attendance_on_this_date_button"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="11dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginStart="21dp"
        android:layout_marginEnd="22dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/view_students_who_attended_button"
        android:layout_marginLeft="21dp"
        android:layout_marginRight="22dp"
        app:layout_constraintHorizontal_bias="0.739" />

    <TextView
        android:id="@+id/total_attendance_on_this_date_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total attendance on this date:"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:layout_marginStart="75dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="75dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="47dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/total_students_that_missed_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:text="Total students that missed:"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/total_attendance_on_this_date_button"
        android:layout_marginLeft="6dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/total_attendance_on_this_date_button" />

    <TextView
        android:id="@+id/percentage_of_class_that_attended_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Percentage of class that attended:"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:layout_marginStart="64dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="64dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="22dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/view_students_who_attended_button"
        android:layout_width="0dp"
        android:layout_height="110dp"
        android:text="View students who attended"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintRight_toLeftOf="@+id/view_students_who_missed_button"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="26dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/view_students_who_missed_button"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="26dp"
        android:layout_marginRight="26dp" />

    <Button
        android:id="@+id/view_students_who_missed_button"
        android:layout_width="153dp"
        android:layout_height="106dp"
        android:text="View students who missed "
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="26dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="26dp" />

    <Button
        android:id="@+id/home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="home"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="38dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="0dp"
        android:layout_height="18dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="9dp"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/date_TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/total_attendance_on_this_date_button"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/total_attendance_on_this_date_button"
        android:layout_marginLeft="7dp"
        android:layout_marginRight="9dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="87dp"
        android:layout_height="19dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/total_students_that_missed_button"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/total_students_that_missed_button" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="20dp"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView1"
        app:layout_constraintBaseline_toBaselineOf="@+id/percentage_of_class_that_attended_button"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/percentage_of_class_that_attended_button" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="139dp"
        android:layout_height="0dp"
        android:text="Attendance on date:"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/date_TextView"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/date_TextView"
        android:layout_marginLeft="32dp" />

</android.support.constraint.ConstraintLayout>
