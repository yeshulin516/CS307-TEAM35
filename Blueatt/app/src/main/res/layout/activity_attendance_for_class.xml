<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.moonsunhwang.blueatt.Attendance_for_class">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Classes Missed: 2"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="48dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Classes Attended: 10"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="125dp"
        tools:layout_editor_absoluteY="16dp" />

    <CalendarView
        android:id="@+id/calendarView2"
        android:layout_width="300dp"
        android:layout_height="297dp"
        tools:layout_editor_absoluteY="74dp"
        tools:layout_editor_absoluteX="42dp" />

    <Button
        android:id="@+id/class_was_missed_button"
        android:layout_width="239dp"
        android:layout_height="54dp"
        android:text="Class was missed"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="441dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="239dp"
        android:layout_height="54dp"
        android:text="Class was attended"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="379dp" />
</android.support.constraint.ConstraintLayout>
