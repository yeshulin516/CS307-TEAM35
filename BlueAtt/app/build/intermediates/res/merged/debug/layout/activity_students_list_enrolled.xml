<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.moonsunhwang.blueatt.Students_list_enrolled">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="176dp"
        android:layout_height="47dp"
        android:text='Select Student(s) to Modify\n "List of Students Enrolled"'
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/checkBox2"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="35dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="25dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:checked="true"
        android:text="Kate"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/checkBox3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox3"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/checkBox3"
        app:layout_constraintHorizontal_bias="1.0" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="272dp"
        android:layout_height="26dp"
        android:text="Jane"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/view_selected_students_attendance_trend_button"
        app:layout_constraintRight_toRightOf="@+id/checkBox2"
        android:layout_marginTop="8dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="54dp"
        app:layout_constraintLeft_toLeftOf="@+id/checkBox2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintTop_toBottomOf="@+id/checkBox2" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:checked="true"
        android:text="Adam"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="58dp"
        android:layout_marginEnd="58dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="134dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="58dp"
        android:layout_marginRight="58dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/remove_selected_from_the_course_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Remove selected from the course"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="61dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="61dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="33dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="61dp"
        android:layout_marginRight="61dp" />

    <Button
        android:id="@+id/view_selected_students_attendance_trend_button"
        android:layout_width="0dp"
        android:layout_height="114dp"
        android:text="View selected students'\n attendance trend"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/remove_selected_from_the_course_button"
        app:layout_constraintRight_toRightOf="@+id/remove_selected_from_the_course_button"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="56dp"
        app:layout_constraintLeft_toLeftOf="@+id/remove_selected_from_the_course_button"
        app:layout_constraintHorizontal_bias="0.0" />

</android.support.constraint.ConstraintLayout>
