<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.moonsunhwang.blueatt.View_my_attendance_report">

    <Button
        android:id="@+id/class1"
        android:layout_width="121dp"
        android:layout_height="79dp"
        android:text="Class 1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/class3"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/class3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/class3"
        app:layout_constraintVertical_bias="0.619" />

    <Button
        android:id="@+id/class2"
        android:layout_width="129dp"
        android:layout_height="82dp"
        android:text="class 2"
        android:layout_marginTop="47dp"
        android:layout_marginLeft="32dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="32dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/class1" />

    <Button
        android:id="@+id/class3"
        android:layout_width="120dp"
        android:layout_height="88dp"
        android:text="class 3"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/class5"
        android:layout_marginStart="49dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="28dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="49dp" />

    <Button
        android:id="@+id/class4"
        android:layout_width="0dp"
        android:layout_height="88dp"
        android:text="class 4"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/class2"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/class2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/class2"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/class5"
        android:layout_width="0dp"
        android:layout_height="88dp"
        android:text="class 5"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/class3"
        app:layout_constraintBaseline_toBaselineOf="@+id/class6"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/class3" />

    <Button
        android:id="@+id/class6"
        android:layout_width="0dp"
        android:layout_height="88dp"
        android:text="class 6"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/class4"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/class4"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/class4" />

</android.support.constraint.ConstraintLayout>
